{
  "name": "@azure-tools/codegen",
  "version": "2.9.1",
  "description": "Autorest Code generator common and base classes",
  "directories": {
    "doc": "docs"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "watch": "tsc -p tsconfig.build.json --watch",
    "build": "tsc -p tsconfig.build.json",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "lint": "eslint ./src --ext .ts --max-warnings=0",
    "prepare": "npm run build",
    "test": "jest --watchAll --coverage=false",
    "test:ci": "jest --ci",
    "clean": "rimraf ./dist ./temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/autorest.git"
  },
  "keywords": [
    "AutoRest",
    "codegen"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/autorest/issues"
  },
  "homepage": "https://github.com/Azure/autorest/tree/main/packages/libs/codegen#readme",
  "readme": "https://github.com/Azure/autorest/tree/main/packages/libs/codegen/readme.md",
  "devDependencies": {
    "@types/jest": "^28.1.0",
    "@types/js-yaml": "~4.0.0",
    "@types/node": "~14.14.20",
    "@types/semver": "5.5.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint-plugin-jest": "~26.4.6",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-unicorn": "~42.0.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^28.0.3",
    "typescript": "~4.7.2"
  },
  "dependencies": {
    "@azure-tools/async-io": "~3.0.0",
    "js-yaml": "~4.0.0",
    "semver": "^7.3.5"
  }
}
